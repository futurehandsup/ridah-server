<% include ../templates/head.ejs %>

<section>
  <div class="container-fluid">
    <p class="mb-3 subtitle">프로그램을 간편하게 등록하고 관리할 수 있습니다</p>
  </div>

  <div class="card" style="margin-bottom: 20px;">
    <table class="table table-default table-hover table-bordered">
      <thead>
        <th>가맹점 공지 세부</th>
      </thead>
      <tbody>
        <% %>
        <% for(var i=0;i<notices.length;i++){
          var notice = notices[i];
        %>
          <tr>
            <td class="info-head">공지제목</td>
            <td class="info-info"><%=notice.noticeTitle%></td>
          </tr>
          <tr>
            <td class="info-head">공지내용</td>
            <td class="info-info"><%=notice.noticeContents%></td>
          </tr>
          <tr>
            <td class="info-head">작성자</td>
            <td class="info-info"><%=notice.noticeWriter%></td>
          </tr>
          <tr>
            <td class="info-head">등록일</td>
            <td class="info-info"><%=notice.createDate%></td>
          </tr>
          <tr>
            <td class="info-head">최종수정일</td>
            <td class="info-info"><%=notice.updateDate%></td>
          </tr>
          <% } %>
      </tbody>
    </table>
  </div>
</section>

<% include ../templates/bottom.ejs %>
